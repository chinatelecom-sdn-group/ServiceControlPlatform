        <ul class="nav navbar-nav pull-right">
            <li>
                <a href="#" style="background-color: #FFF;
						box-shadow: inset 0px -3px 0px #2980B9,
						inset -1px 1px 1px #FFF;" ><span class="badge">50</span></a>
            </li>
        </ul>
        -->
    </div>
    <div id="body-content" style=" height: 100%;">
    <div class="row">
        <!--系统菜单-->
        <div class="col-sm-2">
            <ul class="nav nav-sidebar" id="id_left_menu">
                <li class="nav-header active">
                    &nbsp;
                </li>
                <% for(var i=0; i<nav_head.menu_items.length; i++) { %>
                <%
                    var sub_menu = nav_head.menu_item_links[i];
                    var ul_name = "id_left_menu_ul_"+i;
                %>
                <li class="active">
                    <a href='#'>
                        <span class="glyphicon <%= nav_head.menu_item_icons[i] %>"></span>
                        <%= sub_menu.menu_head %>
                    </a>
                    <ul class="nav" id="<%= ul_name %>">
                        <% for(var j=0; j<sub_menu.menu_items.length; j++) { %>
                        <li id="id_left_menu_sub">
                            <a href='<%= sub_menu.menu_item_links[j] %>'>
                                <%if(sub_menu.menu_items[j]==content.title){%>
                                <span class="glyphicon glyphicon-chevron-right"></span>
                                <%}else{%>
                                <label>&nbsp;&nbsp;&nbsp;&nbsp;</label>
                                <%}%>
                                <%= sub_menu.menu_items[j] %>
                            </a>
                        </li>
                        <% } %>
                    </ul>
                </li>
                <% } %>
            </ul>
        </div>
        <!--系统主页面-->
        <div class="col-sm-10 main">
            <div class="row">
                <ol class="breadcrumb">
                    <li><a href="/project_config" style="color: #ffffff;"><%=nav_head.main_page%></a></li>
                    <li><a href="<%=head_active%>" style="color: #ffffff;"><%=nav_left.menu_head%></a></li>
                    <li class="active" style="color: #ffffff;"><%= content.title %></li>
                </ol>
            </div>
