<style type="text/css">
    .loading{
        width:50px;
        height:56px;
        position: absolute;
        top:30%;
        left:50%;
        line-height:50px;
        color:#fff;
        padding-left:60px;
        font-size:15px;
        background: #fff url(images/loading.gif) no-repeat 50%;
        opacity: 0.7;
        z-index:9999;
        -moz-border-radius:20px;
        -webkit-border-radius:20px;
        border-radius:20px;
        filter:progid:DXImageTransform.Microsoft.Alpha(opacity=70);
    }
    .btn-gsta-primary {
        color: #fff;
        background-color: #44a3e4;
        border-color: #44a3e4;
    }
    .btn-gsta-danger {
        color: #fff;
        background-color: #f88742;
        border-color: #f88742;
    }
    .navbar-gsta-inverse {
        background-color: #428bca;
        border-color: #fff;
    }
    .navbar-gsta-inverse .navbar-brand {
        color: #fff;
    }
    .navbar-gsta-inverse .navbar-brand:hover,
    .navbar-gsta-inverse .navbar-brand:focus {
        color: #f88742;
        background-color: transparent;
    }
    .navbar-gsta-inverse .navbar-text {
        color: #777;
    }
    .navbar-gsta-inverse .navbar-nav > li > a {
        color: #fff;
        margin-top: 5px;
        margin-bottom: 5px;
        line-height: 5px;
    }
    .navbar-gsta-inverse .navbar-nav > li > a:hover,
    .navbar-gsta-inverse .navbar-nav > li > a:focus {
        color: #fff;
        background-color: #4f9dca;
        border-radius: 4px;
    }
    .navbar-gsta-inverse .navbar-nav > .active > a,
    .navbar-gsta-inverse .navbar-nav > .active > a:hover,
    .navbar-gsta-inverse .navbar-nav > .active > a:focus {
        color: #fff;
        background-color: #f88742;
        background-color: #44a3e4;
        border-radius: 4px;
    }
    .navbar-gsta-inverse .navbar-nav > .disabled > a,
    .navbar-gsta-inverse .navbar-nav > .disabled > a:hover,
    .navbar-gsta-inverse .navbar-nav > .disabled > a:focus {
        color: #ccc;
        background-color: transparent;
    }
    .navbar-gsta-inverse .navbar-toggle {
        border-color: #44a3e4;
    }
    .navbar-gsta-inverse .navbar-toggle:hover,
    .navbar-gsta-inverse .navbar-toggle:focus {
        background-color: #44a3e4;
    }
    .navbar-gsta-inverse .navbar-toggle .icon-bar {
        background-color: #888;
    }
    .navbar-gsta-inverse .navbar-collapse,
    .navbar-gsta-inverse .navbar-form {
        border-color: #44a3e4;
    }
    .navbar-gsta-inverse .navbar-nav > .open > a,
    .navbar-gsta-inverse .navbar-nav > .open > a:hover,
    .navbar-gsta-inverse .navbar-nav > .open > a:focus {
        color: #ffffff;
        background-color: #44a3e4;
    }
    @media (max-width: 767px) {
        .navbar-gsta-inverse .navbar-nav .open .dropdown-menu > li > a {
            color: #44a3e4;
        }
        .navbar-gsta-inverse .navbar-nav .open .dropdown-menu > li > a:hover,
        .navbar-gsta-inverse .navbar-nav .open .dropdown-menu > li > a:focus {
            color: #44a3e4;
            background-color: transparent;
        }
        .navbar-gsta-inverse .navbar-nav .open .dropdown-menu > .active > a,
        .navbar-gsta-inverse .navbar-nav .open .dropdown-menu > .active > a:hover,
        .navbar-gsta-inverse .navbar-nav .open .dropdown-menu > .active > a:focus {
            color: #2980B9;
            background-color: #3498DB;
        }
        .navbar-gsta-inverse .navbar-nav .open .dropdown-menu > .disabled > a,
        .navbar-gsta-inverse .navbar-nav .open .dropdown-menu > .disabled > a:hover,
        .navbar-gsta-inverse .navbar-nav .open .dropdown-menu > .disabled > a:focus {
            color: #ccc;
            background-color: transparent;
        }
    }
    .navbar-gsta-inverse .navbar-link {
        color: #3498DB;
    }
    .navbar-gsta-inverse .navbar-link:hover {
        color: #2980B9;
    }
    .navbar-gsta-inverse .btn-link {
        color: #3498DB;
    }
    .navbar-gsta-inverse .btn-link:hover,
    .navbar-gsta-inverse .btn-link:focus {
        color: #2980B9;
    }
    .navbar-gsta-inverse .btn-link[disabled]:hover,
    fieldset[disabled] .navbar-gsta-inverse .btn-link:hover,
    .navbar-gsta-inverse .btn-link[disabled]:focus,
    fieldset[disabled] .navbar-gsta-inverse .btn-link:focus {
        color: #ccc;
    }
